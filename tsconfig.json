{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": false,
      "module": "umd",
      "target": "es6",
      "downlevelIteration": true,
      "jsx": "react",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "strictNullChecks": false,
      "strictFunctionTypes": false,
      "moduleResolution": "node"
    }
}