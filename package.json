{
  "name": "vscode-uibuilder-webview-angular",
  "version": "0.0.0",
  "displayName": "UI Builder Webview",
  "description": "UI builder Webview App for your next VSCode extension based on Angular",
  "publisher": "ClixUIBuilder",
  "enableProposedApi": true,
  "engines": {
    "vscode": "^1.41.0"
  },
  "main": "./dist/ext-src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "uibuilder-webview",
          "title": "UI Builder",
          "icon": "media/extension-icon.png"
        }
      ]
    },
    "views": {
      "uibuilder-webview": [
        {
          "id": "uibuilder-webview",
          "name": "Webview"
        }
      ]
    },
    "commands": [
      {
        "command": "uibuilder-webview.openWebview",
        "title": "Open UI Builder",
        "category": "Webview",
        "icon": {
          "light": "media/extension-icon.png",
          "dark": "media/extension-icon.png"
        }
      }
    ]
  },
  "activationEvents": [
    "onCommand:uibuilder-webview.openWebview",
    "onView:uibuilder-webview"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack ./ext-src/extension.ts --output-filename=extension.js --mode=development && tsc -p tsconfig.extension.json",
    "package": "vsce package --yarn",
    "vscode:prepublish": "npm run build && tsc -p tsconfig.extension.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^15.12.2",
    "fs": "0.0.1-security",
    "node-dir": "^0.1.17",
    "path": "^0.12.7",
    "ts-file-parser": "0.0.19",
    "vscode-test": "^1.5.2",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.0.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@types/vscode": "^1.41.0",
    "ts-loader": "^9.0.2",
    "typescript": "^4.2.4",
    "vsce": "^1.71.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/4gray/vscode-webview-angular.git"
  }
}
